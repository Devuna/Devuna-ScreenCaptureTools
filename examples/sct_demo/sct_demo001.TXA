WHEN '?FilePrintSetup'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
IF sav:szPrinterName = ''
  sav:szPrinterName = GETINI('windows','device')
END
IF PRINTERDIALOG()
  szPrinterName = PRINTER{PROPPRINT:Device} & ',' & |
                  GETINI('PrinterPorts',PRINTER{PROPPRINT:Device})
  PUTINI('windows','device', szPrinterName)
END

