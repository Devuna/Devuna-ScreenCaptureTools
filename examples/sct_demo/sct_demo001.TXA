EMBED %WindowManagerMethodCodeSection
[INSTANCES]
WHEN 'TakeEvent'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 6300
PROPERTY:END
  CASE EVENT()
  OF EVENT:StartAbout
     START(About,25000)
  OF EVENT:StartClipboardDemo
     START(ClipboardDemo,25000)
  END
[END]
[END]
WHEN 'Kill'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 8500
PROPERTY:END
IF sav:szPrinterName <> ''
  PUTINI('windows','device', sav:szPrinterName)
END
[END]
[END]
[END]
