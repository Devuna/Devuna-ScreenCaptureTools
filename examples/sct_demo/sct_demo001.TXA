AppFrame APPLICATION('Devuna Screen Capture Tools'),AT(,,310,192),RESIZE,MAXIMIZE,CENTERED, |
          ICON('sctools.ico'),MAX,STATUS,SYSTEM
          MENUBAR,USE(?MENUBAR1),#ORDINAL(1)
            ITEM('E&xit'),USE(?Exit),MSG('Exit this application'),STD(STD:Close),#ORDINAL(2)
            MENU('&Print Window'),USE(?FilePrint),MSG('Demonstrates the various Print W' & |
              'indow Functions'),#ORDINAL(3)
              ITEM('&Desktop'),USE(?FilePrintDesktop),MSG('Print Desktop'),#ORDINAL(4)
              ITEM('&Application'),USE(?FilePrintApplication),MSG('Print Application'),#ORDINAL(5)
              ITEM('&Client Area'),USE(?FilePrintClientArea),MSG('Print Application Client Area'), |
                #ORDINAL(6)
              ITEM('&Window'),USE(?FilePrintWindow),MSG('Print Top Client Window'),#ORDINAL(7)
              ITEM('&Select Area'),USE(?FilePrintSelectArea),MSG('Print Selected Screen Area'), |
                #ORDINAL(8)
              ITEM('Clip&board'),USE(?FilePrintClipboard),#ORDINAL(9)
              ITEM,USE(?SEPARATOR1),SEPARATOR,#ORDINAL(10)
              ITEM('S&etup...'),USE(?FilePrintSetup),MSG('Setup Printer'),#ORDINAL(11)
            END
            MENU('&Save Window'),USE(?FileSave),MSG('Demonstrates the various Save Wind' & |
              'ow Functions'),#ORDINAL(12)
              ITEM('&Desktop'),USE(?FileSaveDesktop),MSG('Save Desktop to BMP'),#ORDINAL(13)
              ITEM('&Application'),USE(?FileSaveApplication),MSG('Save Application to BMP'), |
                #ORDINAL(14)
              ITEM('&Client Area'),USE(?FileSaveClientArea),MSG('Save Application Clien' & |
                't Area to BMP'),#ORDINAL(15)
              ITEM('&Window'),USE(?FileSaveWindow),MSG('Save Top Client Window to BMP'),#ORDINAL(16)
              ITEM('&Select Area'),USE(?FileSaveSelectArea),MSG('Print Selected Screen ' & |
                'Area to BMP'),#ORDINAL(17)
              ITEM('Clip&board'),USE(?FileSaveCilpboard),#ORDINAL(18)
            END
          END
        END

